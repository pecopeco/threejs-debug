{
  "name": "threejs-debug",
  "version": "1.1",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "assets/logo.png",
      "48": "assets/logo.png",
      "128": "assets/logo.png"
    },
    "default_title": "Popup Sample",
    "default_popup": "./views/popup.html"
  },
  "icons": {
    "16": "assets/logo.png",
    "48": "assets/logo.png",
    "128": "assets/logo.png"
  },
  "background": {
    "service_worker": "./views/background.js",
    "type": "module"
  },
  "devtools_page": "./views/devtools.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": ["./views/content_scripts.js"]
    }
  ],
  "permissions": [
    "identity", "storage", "tabs"
  ]
}
